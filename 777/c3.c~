  printf("I*\n");
  printf("I**\n");
  printf("I****\n");
  printf("I******\n");
  printf("I********\n");
  printf("I**********\n");
  printf("I************\n");
  printf("I*************\n");
  printf("I\n");
  printf("I\n");
  printf("I\n");
  printf("I\n");
  printf("I\n");
  printf("I\n");
  printf("I\n");
  printf("I\n");
  printf("\533[0m"); //去除颜色
	 return 0;
}
